{
  "services": {
    "canvas_production": {
      "type": "canvas",
      "name": "Canvas LMS",
      "config": {
        "base_url": "https://your-school.instructure.com",
        "api_key": "your_canvas_api_key",
        "timeout": 30,
        "retry_attempts": 3,
        "rate_limit_per_minute": 100
      }
    },
    "moodle_production": {
      "type": "moodle",
      "name": "Moodle LMS",
      "config": {
        "base_url": "https://your-school.edu/moodle",
        "api_key": "your_moodle_token",
        "timeout": 30,
        "retry_attempts": 3,
        "rate_limit_per_minute": 100
      }
    }
  }
}
